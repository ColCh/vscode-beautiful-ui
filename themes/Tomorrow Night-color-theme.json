	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#959795"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"variable.other",
